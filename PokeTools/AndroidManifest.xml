<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tim.poketools"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tim.poketools.MainActivity"
            android:label="@string/title_activity_main"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name= "com.tim.user.MemberActivity"
            android:label="@string/title_activity_members"
            />
        <activity 
            android:name= "com.tim.user.AliasActivity"
            android:label="@string/title_activity_alias"
            />
        <activity 
            android:name= "com.tim.common.RaceActivity"
            android:label="@string/title_activity_race"
            />
        <activity 
            android:name= "com.tim.common.TypeActivity"
            android:label="@string/title_activity_type"
            />
        <activity 
            android:name= "com.tim.common.MoveView"
            android:label="@string/title_activity_move"
            />
    </application>

</manifest>